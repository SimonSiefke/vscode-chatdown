{
  "files.associations": {
    "*.json": "jsonc",
    "*.bot": "json"
  },
  "json.schemas": [
    {
      "fileMatch": ["/syntaxes/json2.json"],
      "url": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json"
    },
    { "fileMatch": ["*.bot"], "url": "/schemas/bot.json" }
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json": "chatdown.yml"
  }
}
